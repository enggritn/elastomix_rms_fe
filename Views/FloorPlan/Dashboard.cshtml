<script>

        var URL_API = '@System.Configuration.ConfigurationManager.AppSettings["server"].ToString()';

        //$('.nav-link').hide();
        $('#select-warehouse').change(function () {
            $('.nav-link').hide();

            var WAREHOUSE = $(this).val();
            if (WAREHOUSE === 'RAW_MATERIAL') {
                $('.tab-RAW_MATERIAL').show();
            }

            if (WAREHOUSE === 'SEMI_FINISH_GOOD') {
                $('.tab-SEMI_FINISH_GOOD').show();
            }


            $('.tab-pane').removeClass('in');
            $('.tab-pane').removeClass('active');
            $('.tab-pane').removeClass('show');

            $('.tab-pane').addClass('fade');


        })

        $('#menu-togel').click(function () {
            $('#menu-layout').slideDown();
            $('#user-tracking').slideUp();
            $('.layout-table').hide();
            $('#title-layout').text('Dashboard Layout Warehouse');
        })

    let area = [];
    let areaType = "";
    let isDone = false;
    let isDone1 = false;
        $('.btn-layout-show ').click(function () {
            $('#menu-layout').slideUp();
            $('.layout-table').hide();
            var idLayout = $(this).attr('href');
            $(idLayout).show();
            $('#user-tracking').slideDown();

            var title = '';
            switch (idLayout) {
                case "#FACTORY1":
                    title = 'LAYOUT RAW MATERIAL - FACTORY 1';
                    area = ["AR001", "AR004"];
                    areaType = "LOGISTIC";
                    refreshLayout();
                    refreshUser();
                    setInterval(function () {
                        if (isDone) {
                            refreshLayout();
                        }

                        if (isDone1) {
                            refreshUser();
                        }

                    }, 3000);
                    break;
                case "#RM_TENT":
                    title = 'LAYOUT RAW MATERIAL - RM TENT';
                    area = ["AR002", "AR005"];
                    areaType = "LOGISTIC";
                    refreshLayout();
                    refreshUser();
                    setInterval(function () {
                        if (isDone) {
                            refreshLayout();
                        }

                        if (isDone1) {
                            refreshUser();
                        }

                    }, 3000);
                    break;

                case "#QC_AREA":
                    title = 'LAYOUT OTHER - QC AREA';
                    break;

                case "#FG_TENT":
                    title = 'LAYOUT FINISH GOOD - FG TENT';
                    area = ["AR009", "AR003"];
                    areaType = "LOGISTIC";
                    refreshLayout();
                    refreshUser();
                    setInterval(function () {
                        if (isDone) {
                            refreshLayout();
                        }

                        if (isDone1) {
                            refreshUser();
                        }

                    }, 3000);
                    break;

                case "#WG_CHEMICAL":
                    title = 'RACK WEIGHING CHEMICAL';
                    area = ["AR008"];
                    areaType = "PRODUCTION";
                    refreshLayout();
                    refreshUser();
                    setInterval(function () {
                        if (isDone) {
                            refreshLayout();
                        }

                        if (isDone1) {
                            refreshUser();
                        }

                    }, 3000);
                    break;

                case "#WIP":
                    title = 'LAYOUT RACK PRODUKSI';
                    area = ["AR010", "AR011", "AR012", "AR013", "AR015", "AR018"];
                    areaType = "PRODUCTION";
                    refreshLayout();
                    refreshUser();
                    setInterval(function () {
                        if (isDone) {
                            refreshLayout();
                        }

                        if (isDone1) {
                            refreshUser();
                        }

                    }, 3000);
                    break;

                default:
                    title = 'Dashboard Layout Warehouse';
                    break;
            }

            $('#title-layout').text(title);


        })





        var table = $('#item-list').DataTable({
            processing: true,
            serverSide: true,
            ajax: {
                dataType: "json",
                type: "POST",
                url: URL_API + "api/floor-plan/data-table",
                data: function (d) {
                    d.extra_search = $('#extra').val();
                }
            },
            columns: [
                { "data": "MaterialCode", "name": "MaterialCode", "autoWidth": false },
                { "data": "MaterialName", "name": "MaterialName", "autoWidth": false },
                { "data": "LotNumber", "name": "LotNumber", "autoWidth": false },
                { "data": "InDate", "name": "InDate", "autoWidth": false },
                { "data": "ExpDate", "name": "ExpDate", "autoWidth": false },
                { "data": "QtyBag", "name": "QtyBag", "autoWidth": false },
                { "data": "QtyPerBag", "name": "QtyPerBag", "autoWidth": false },
                { "data": "Qty", "name": "Qty", "autoWidth": false },
                {
                    "data": null,
                    "render": function (data, type, row) {
                        let badge = '';
                        if (!data.IsExpired) {
                            badge = '<span class="badge badge-success">Not Expired</span>';
                        } else {
                            badge = '<span class="badge badge-danger">Expired</span>';
                        }
                        return badge;
                    },
                    "name": "IsExpired"
                },
                {
                    "data": null,
                    "render": function (data, type, row) {
                        let badge = '';
                        if (!data.OnInspect) {
                            badge = '<span class="badge badge-success">Available</span>';
                        } else {
                            badge = '<span class="badge badge-danger">On QC</span>';
                        }
                        return badge;
                    },
                    "name": "OnInspect"
                },
            ]
        });


        //$("#form-lookup").on('shown', function () {
        //    alert("I want this to appear after the modal has opened!");
        //});

        $('.btn-info-rak').click(function () {
            var rakArea = $(this).data('area');
            var rakLabel = $(this).data('rak');

            var label = '';

            if (rakLabel !== undefined && rakLabel !== null && rakLabel !== '') {

                if (rakArea !== undefined && rakArea !== null && rakArea !== '') {
                    label += rakArea + ' - ' + rakLabel;
                } else {
                    label += rakLabel;
                }

            }
            table.clear().draw();


            $('#confirmationModalTitle').text(label);
            //call interval here++

            table.ajax.url(URL_API + "api/floor-plan/data-table?binrack=" + rakArea + rakLabel).load();
            $('.dataTables_filter').find('input').val('');

            $('#form-lookup').modal('show');

        });




        $('.btn-next-rak').click(function () {
            $(".table-wip-1").animate({ left: '250px' });
            $('.table-wip-1').hide();
            $('.table-wip-2').show();
        })

        $('.btn-prev-rak').click(function () {
            $(".table-wip-2").animate({ right: '250px' });
            $('.table-wip-2').hide();
            $('.table-wip-1').show();
        })

    function refreshLayout() {
            $.ajax({
                type: 'POST',
                dataType: "json",
                data: { AreaCode: area },
                url: URL_API + 'Api/BinRack/GetRackItems',
                success: function (response) {
                    $.each(response.data, function (index, value) {
                        let button = $("table").find('#' + value.BinRackCode)
                        let table = $('#' + value.BinRackCode).closest('table').attr('id');
                        if (value.TotalQty == 0) {
                            //let button = $("table").find('[data-area="' + areaCode + '"][data-rak="' + value.BinRackName + '"]')
                            if (button) {

                                $(button).addClass("empty-rack");
                                //check btn color
                                //if ($(button).hasClass("btn-primary")) {
                                //    $(button).removeClass("btn-primary");
                                //}

                                //if ($(button).hasClass("btn-danger")) {
                                //    $(button).removeClass("btn-danger");
                                //}

                                //if ($(button).hasClass("btn-warning")) {
                                //    $(button).removeClass("btn-warning");
                                //}

                                //if ($(button).hasClass("btn-success")) {
                                //    $(button).removeClass("btn-success");
                                //}

                                //$(button).addClass("btn-default");
                            } else {
                                console.log("rack not found : " + value.BinRackCode);
                            }

                        } else {
                            $(button).removeClass("empty-rack");
                        }

                    });

                    isDone = true;
                }
            });
    }

    function refreshUser() {

        $.ajax({
            type: 'GET',
            dataType: "json",
            data: { AreaType: areaType },
            url: URL_API + 'Api/User/GetMobileUsers',
            success: function (response) {
                let html = '<div class="media text-muted pt-3">';
                html += '<div class="media-body pb-3 mb-0 small lh-125 border-bottom border-gray">';
                html += '<div class="d-flex justify-content-between align-items-center w-100">';
                html += '<strong class="text-gray-dark">No User</strong>';
                html += '</div>';
                html += '</div>';
                html += '</div>';

                if (response.data.length > 0) {
                    html = '';

                    $.each(response.data, function (index, value) {

                        html += '<div class="media text-muted pt-3">';
                        html += '<img src = "' + URL_API + value.Photo + '" alt = "" class="mr-2 rounded" data - holder - rendered="true" style = "width: 48px; height: 48px;" >';
                        html += '<div class="media-body pb-3 mb-0 small lh-125 border-bottom border-gray">';
                        html += '<div class="d-flex justify-content-between align-items-center w-100">';
                        html += '<strong class="text-gray-dark">' + value.FullName + '</strong>';
                        html += '</div>';
                        if (value.LoginStatus) {
                            html += '<span class="d-block text-success">Online</span>';
                        } else {
                            html += '<span class="d-block text-warning">Offline</span>';
                        }

                        html += '</div>';
                        html += '</div >';

                    });

                }


                $('#list_user').html(html);

                isDone1 = true;
            }
        });
    }

</script>

