
@{
    ViewBag.Title = "Receiving WIP";
    ViewBag.Module = "ReceivingSFG";
}

<div class="page-header">
    <div class="row align-items-end">
        <div class="col-lg-8">
            <div class="page-header-title">
                <i class="ik ik-user bg-blue"></i>
                <div class="d-inline">
                    <h5>@ViewBag.Title</h5>
                    <span>List</span>
                </div>
            </div>
        </div>
        <div class="col-lg-4">
            <nav class="breadcrumb-container" aria-label="breadcrumb">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item">
                        <a href=""><i class="ik ik-database"></i></a>
                    </li>
                    <li class="breadcrumb-item">
                        <a href="#">Warehouse</a>
                    </li>
                    <li class="breadcrumb-item active" aria-current="page">@ViewBag.Title</li>
                </ol>
            </nav>
        </div>
    </div>
</div>

<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-body">
                <div class="form-group row">
                    <label class="col-form-label col-sm-1">Import Data</label>
                    <div class="col-sm-3">
                        <div class="custom-file">
                            @Html.TextBox("file_upload", "", new { @type = "file", @class = "custom-file-input", @id = "file_upload", @accept = "application/vnd.ms-excel, application/vnd.openxmlformats-officedocument.spreadsheetml.sheet" })
                            <label class="custom-file-label" for="customFile">Choose file</label>
                        </div>
                        <span class="text-muted text-sm"><i>Allowed file type : .xls/ .xlsx</i></span>
                    </div>
                    <div class="col-sm-4">
                        <button type="button" class="btn btn-warning" onclick="resetUpload()"><i class="ik ik-delete"></i>Cancel</button>
                        <button type="button" class="btn btn-primary" onclick="uploadData()"><i class="ik ik-upload"></i>Import Data</button>
                    </div>
                </div>
                @*<div class="form-group row">
                        <label class="col-form-label col-sm-1">Warehouse</label>
                        <div class="col-sm-3">
                            <select class="form-control" id="WarehouseFilter">
