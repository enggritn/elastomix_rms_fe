    function PrintBarcode() {
            $("#error_BarcodeListPrint").html("");
            $("#error_MaterialCode").html("");
            $("#error_Printer").html("");
            $("#error_Qty").html("");
            $.ajax({
                type: 'POST',
                dataType: "json",
                beforeSend: function (request) {
                    request.setRequestHeader('Token', token);
                },
                url: server + 'Api/ReceivingSFG/Print',
                data: {
                    LastSeriesBarcode: $('#LastSeriesBarcode').val(),
                    StockCode: $('#BarcodeListPrint').val(),
                    PrintQty: $('#PrintQty').val() != "" ? $('#PrintQty').val().replace(/\,/g, '') : 0,
                    UseSeries: $('#PrintSeries').prop('checked'),
                    Printer: $('#PrinterList').find(":selected").val(),
                },
                success: function (response) {
                    isValid = response.status;
                    if (!isValid) {
                        $.each(response.error_validation, function (index, value) {
                            if (value.FieldName == "PrintRawMateCode") {
                                $('#error_MaterialCode').html(value.ErrorMessage);
                                $('#error_MaterialCode').addClass('is-invalid');
                            } else if (value.FieldName == "PrinterList") {
                                $('#error_Printer').html(value.ErrorMessage);
                                $('#error_Printer').addClass('is-invalid');
                            } else if (value.FieldName == "BarcodeListPrint") {
                                $('#error_BarcodeListPrint').html(value.ErrorMessage);
                                $('#error_BarcodeListPrint').addClass('is-invalid');
                            } else if (value.FieldName == "PrintQty") {
                                $('#error_Qty').html(value.ErrorMessage);
                                $('#error_Qty').addClass('is-invalid');
                            } else {
                                $('#' + value.FieldName).next().html(value.ErrorMessage);
                                $('#' + value.FieldName).addClass('is-invalid');
                            }
                        });
                        notify("Warning", "warning", response.message);

                    } else {
                        notify("Success", "success", response.message);
                        setTimeout(function () { window.location.reload() }, 1000);
                    }
                },
                error: function (xhr, ajaxOptions, thrownError) {
                    notify("Error", "error", "Oops! Something went wrong.");
                },
                complete: function () {
                    $('#saveModal').modal('hide');
                    $('#btnSave').prop('disabled', false);
                }

            });
        }

        $(".custom-file-input").on("change", function () {
            var fileName = $(this).val().split("\\").pop();
            if (fileName)
                $(this).siblings(".custom-file-label").addClass("selected").html(fileName);
            else
                resetUpload();
        });

        function resetUpload() {
            $('#file_upload').val('');
            $('.custom-file-label').html('Choose file');
        }

        function uploadData() {

            let fv = $("#file_upload").val();
            if (fv == undefined || fv == '') {
                notify("Info", "info", "Please choose file.");
                return false;
            }

            $('#uploadModal').modal('show');
        }



        $('#btnUploadSave').click(function () {
            doUpload();
        });

        function doUpload() {
            var formData = new FormData();
            var file = document.getElementById("file_upload").files[0];
            formData.append("file_upload", file);

            $('#btnUploadSave').prop('disabled', true);
            $('#btnUploadSave').text("Please Wait ...");
            //loading animation here
            $.ajax({
                type: 'POST',
                beforeSend: function (request) {
                    request.setRequestHeader('Token', token);
                },
                url: server + 'Api/@ViewBag.Module/Upload',
                data: formData,
                dataType: 'json',
                contentType: false,
                processData: false,
                success: function (response) {
                    if (response.status) {
                        $('#uploadModal').modal('hide');
                        notify("Success", "success", response.message);
                        reloadTable();
                        resetUpload();
                    } else {
                        notify("Warning", "warning", response.message);
                    }
                },
                error: function (xhr, ajaxOptions, thrownError) {
                    notify("Error", "error", "Oops! Something went wrong.");
                },
                complete: function () {
                    $('#btnUploadSave').prop('disabled', false);
                    $('#btnUploadSave').text("Yes");
                }
            });
        }

        function reloadTable() {
            table.ajax.reload();
    }

