@section custom_js
{
    @Scripts.Render("~/Content/plugins/datatables/jquery.dataTables.min.js")
    @Scripts.Render("~/Content/plugins/datatables/dataTables.bootstrap4.min.js")
    @Scripts.Render("~/Content/plugins/datatables/dataTables.fixedColumns.min.js")
    @Scripts.Render("~/Content/plugins/toastr/toastr.min.js")
    @Scripts.Render("~/Content/custom/js/notification.js")
    @Scripts.Render("~/Content/plugins/bootstrap-datepicker/js/bootstrap-datepicker.min.js")
    @Scripts.Render("~/Content/plugins/inputmask/js/jquery.inputmask.bundle.js")
    @Scripts.Render("~/Content/plugins/daterangepicker/moment.min.js")
    @Scripts.Render("~/Content/plugins/select2/dist/js/select2.min.js")




    <script>
        let server = '@ViewBag.server';
        var isValid = true;
        let token = '@Session["token"].ToString()';
        let module = '@ViewBag.Module';
        let baseUrl = '@ViewBag.BaseUrl';

        $('.number').inputmask({ "placeholder": "", 'alias': 'decimal', 'groupSeparator': ',', 'autoGroup': true, 'digits': 2, 'digitsOptional': true});
        $('.number-non-decimal').inputmask({ "placeholder": "", 'alias': 'decimal', 'groupSeparator': ',', 'autoGroup': true, 'digits': 0, 'digitsOptional': false });

        $('#ReceiveOKQty').on('input', function () {

            var ReceiveQtyRakAvail = 0;
            var RemainderQty = 0;

            var ReceiveRequestQtyTotal = parseInt($('#ReceiveRequestQtyTotal').val());
            var ReceiveRequestQtyPerBag = parseFloat($('#ReceiveRequestQtyPerBag').val());
            var ReceiveOKQty = parseFloat(this.value.replace(',', ''));

            ReceiveQtyRakAvail = ReceiveOKQty / ReceiveRequestQtyPerBag;
            RemainderQty = ReceiveOKQty - (parseInt(ReceiveQtyRakAvail) * ReceiveRequestQtyPerBag);

            if (ReceiveRequestQtyTotal / ReceiveRequestQtyPerBag == 1) {
                if (ReceiveOKQty == 0) {
                    $('#ReceiveQtyRakAvail').val('0');
                    $('#RemainderQty').val('0');
                } else {
                    $('#ReceiveQtyRakAvail').val('1');
                    $('#RemainderQty').val('0');
                }
            } else {
                $('#ReceiveQtyRakAvail').val(ReceiveQtyRakAvail);
                $('#RemainderQty').val(RemainderQty);
            }
        });

        $('#JudgementQTY').on('input', function () {

            var ReceiveQtyRakAvail = 0;
            var RemainderQty = 0;

            var ReceiveRequestQtyTotal = parseInt($('#JudgementActualQtyTotal').val());
            var ReceiveRequestQtyPerBag = parseFloat($('#JudgementActualQtyPerBag').val());
            var ReceiveOKQty = parseFloat(this.value.replace(',', ''));


            if ((ReceiveRequestQtyTotal !== undefined && ReceiveRequestQtyTotal !== null && ReceiveRequestQtyTotal > 0) && (ReceiveOKQty !== undefined && ReceiveOKQty !== null && ReceiveOKQty > 0)) {

                if (ReceiveOKQty > ReceiveRequestQtyTotal) {
                    ReceiveOKQty = ReceiveRequestQtyTotal;
                    $(this).val(ReceiveOKQty);
                }

                ReceiveQtyRakAvail = Math.floor(ReceiveOKQty / ReceiveRequestQtyPerBag);
                RemainderQty = ReceiveOKQty - (parseInt(ReceiveQtyRakAvail) * ReceiveRequestQtyPerBag);
            }

            $('#JudgementQtyRakAvail').val(ReceiveQtyRakAvail);
            $('#JudgementRemainderQty').val(RemainderQty);
        });

        $('.datepicker').datepicker({
            format: "dd/mm/yyyy",
            todayBtn: "linked",
            todayHighlight: true,
            orientation: "left",
            autoclose: true,
            startDate: '+0d',
        });

        function hideJudgement() {
            $('#nav-profile').removeClass('show');
            $('#nav-profile').removeClass('active');
        }

        function hideReceive() {
            $('#nav-home').removeClass('show');
            $('#nav-home').removeClass('active');
        }

        function showTabs() {
            $('#nav-home').addClass('show');
            $('#nav-home').addClass('active');
            $('#nav-profile').addClass('show');
            $('#nav-profile').addClass('active');
        }

        var table = $('#data_table').DataTable({
            processing: true,
            serverSide: true,
            sScrollXInner: '150%',
            scrollY: "500px",
            scrollX: true,
            scrollCollapse: true,
            paging: true,
            order: [[1, "desc"]],
            ajax: {
                dataType: "json",
                type: "POST",
                url: server + "Api/@ViewBag.Module/DatatableReceivingGroup",
            },
            initComplete: function (settings, json) {
                $('.number').inputmask({ "placeholder": "", 'alias': 'decimal', 'groupSeparator': ',', 'autoGroup': true, 'digits': 2, 'digitsOptional': true});
                $('.number-non-decimal').inputmask({ "placeholder": "", 'alias': 'decimal', 'groupSeparator': ',', 'autoGroup': true, 'digits': 0, 'digitsOptional': false });
            },
            columnDefs: [
                { orderable: false, targets: 0 },
                { orderable: false, targets: -1 },
            ],
            columns: [

                { "data": "WarehouseCode", "name": "WarehouseCode", "autoWidth": false },
                { "data": "WarehouseName", "name": "WarehouseName", "autoWidth": false },
                { "data": "MaterialRequestCode", "name": "MaterialRequestCode", "autoWidth": false },
                { "data": "ProductCode", "name": "ProductCode", "autoWidth": false },
                { "data": "ProductName", "name": "ProductName", "autoWidth": false },
                { "data": "LotNo", "name": "LotNo", "autoWidth": false },
                { "data": "InDate", "name": "InDate", "autoWidth": false },
                { "data": "ExpDate", "name": "ExpDate", "autoWidth": false },
                { "data": "UoM", "name": "UoM", "autoWidth": false },
                { "data": "QtyPerBag", "name": "QtyPerBag", "autoWidth": false },
                { "data": "TotalOrder", "name": "TotalOrder", "autoWidth": false },
                { "data": "AvailableReceive", "name": "AvailableReceive", "autoWidth": false },
                { "data": "TotalReceive", "name": "TotalReceive", "autoWidth": false },
                {
                    "data": null,
                    "render": function (data, type, row) {
                        //console.log(data.TransactionStatus);
                        let button = '';

                        if (data.EditReceiveAction) {
                            button += '<button class="btn btn-info" onclick = "ShowEditOrderForm(this)" > <i class="ik ik-edit"></i>Edit Order</button> ';
                        }
                        if (data.ReceiveAction) {
                            button += '<button class="btn btn-success" onclick = "ShowReceiveForm(this)" > <i class="ik ik-check-circle"></i>Receive</button> ';
                        }
                        if (data.PrintBarcodeAction) {
                            button += '<button type="button" class="btn btn-primary" value ="press" onclick="return ShowPrintBarcodeForm(this)"><i class="ik ik-inbox"></i>Print Barcode</button> ';
                        }
                        if (data.PutawayAction) {
                            button += '<button class="btn btn-primary" onclick="ShowPutawayForm(this)"><i class="ik ik-inbox"></i>Putaway</button>';
                        }
                        return button;
                    }
                },
            ]
        });

        var all_tr = $('tr');
        $('td input[type="button"]').on('click', function () {
            all_tr.removeClass('selected');
            $(this).closest('tr').addClass('selected');
        });

        function ShowPrintBarcodeForm(row) {
            var elementRow = $(row).closest('tr');
            var dataTableRow = table.row($(elementRow)).data();
            $('#PrintRawMateCode').val(dataTableRow.ProductCode);
            $('#PrintRawMateName').val(dataTableRow.ProductName);
            $('#PrintLotNo').val(dataTableRow.LotNo);
            $('#PrintInDate').val(dataTableRow.InDate);
            $('#PrintExpDate').val(dataTableRow.ExpDate);
            $('#PrintQty').val('0');

            $('#printBarcodeModal').modal({
                backdrop: 'static',
                keyboard: false
            });
            GetBarcodeListPrint();
        }

        setPrinterList();

        function setPrinterList() {
            $('#PrinterList').select2();
            $.ajax({
                type: 'GET',
                async: false,
                dataType: "json",
                beforeSend: function (request) {
                    request.setRequestHeader('Token', token);
                },
                url: server + 'Api/Control/ListPrinterWeb',
                success: function (response) {

                    $('#PrinterList').empty();
                    var html = '';
                    html += '<option value="" selected>Select Printer</option>';
                    $.each(response.printers, function (index, value) {
                        html += '<option value="' + value.PrinterIP + '">' + value.PrinterName + '</option>';
                    });
                    $('#PrinterList').append(html);
                    $('#PrinterList').select2({ width: '100%' });
                }
            });
        }

        function setAreaList() {
            $('#AreaList').select2();
            $.ajax({
                type: 'GET',
                async: false,
                dataType: "json",
                beforeSend: function (request) {
                    request.setRequestHeader('Token', token);
                },
                url: server + 'Api/ReceivingSFG/AreaList',
                success: function (response) {

                    $('#AreaList').empty();
                    var html = '';
                    html += '<option value="" selected>Select Area</option>';
                    $.each(response, function (index, value) {
                        html += '<option value="' + value.ID + '">' + value.Code + ' - ' + value.Name + '</option>';
                    });
                    $('#AreaList').append(html);
                    //$('#AreaList').select2({ width: '100%' });
                }
            });
        }

        function GetBarcodeListPutAway() {
            $.ajax({
                type: 'POST',
                async: false,
                dataType: "json",
                beforeSend: function (request) {
                    request.setRequestHeader('Token', token);
                },
                url: server + 'Api/ReceivingSFG/ListBarcodeSFGPutaway',
                data: {
                    ExpDate: moment(xExpDate, 'DD/MM/YYYY').format('YYYY-MM-DD'),
                    InDate: moment(xInDate, 'DD/MM/YYYY').format('YYYY-MM-DD'),
                    LotNo: xLotNo,
                    ProductCode: xProductCode,
                },

                success: function (response) {
                    $('#tablebarcode tbody').empty();
                    if (response.status) {
                        list_fifo = response.data;
                        var html = "";

                        if (list_fifo.length > 0) {
                            $.each(list_fifo, function (index, value) {
                                html += '<tr>';
                                html += '<td><input type="radio" name="id" onchange="chooseStock(\'' + index + '\')"></td>';

                                //html += '<td>' + value.StockCode + '</td>';
                                html += '<td>' + value.ProductCode + '</td>';
                                html += '<td>' + value.ProductName + '</td>';
                                html += '<td>' + value.LotNo + '</td>';
                                html += '<td>' + value.InDate + '</td>';
                                html += '<td>' + value.ExpDate + '</td>';
                                html += '<td>' + value.UoM + '</td>';
                                html += '<td>' + value.QtyPerBag + '</td>';
                                html += '</tr>';
                            });
                        } else {
                            html += '<tr><td colspan="13">No Data</td></tr>';
                        }


                        $('#tablebarcode tbody').append(html);

                        $('#formPutawayModal').modal({
                            backdrop: 'static',
                            keyboard: false
                        });

                    } else {
                        notify("Warning", "warning", response.message);
                    }
                }
            });
        }

        let xStockCode = "";
        function chooseStock(index) {
            data_fifo = list_fifo[index];
            xStockCode = data_fifo.StockCode;
            $.ajax({
                type: 'GET',
                async: false,
                dataType: "json",
                beforeSend: function (request) {
                    request.setRequestHeader('Token', token);
                },
                url: server + 'Api/ReceivingSFG/GetBarcode?StockCode=' + data_fifo.StockCode,
                success: function (response) {
                    $('#AvailablePutawayQTY').val(response.QtyBagAvailable);
                    $('#PutAwayActualQtyTotal').val(response.QtyActualAvailable);
                }
            });
        }

        function GetBarcodeListPrint() {
            $.ajax({
                type: 'POST',
                async: false,
                dataType: "json",
                beforeSend: function (request) {
                    request.setRequestHeader('Token', token);
                },
                url: server + 'Api/ReceivingSFG/ListBarcodeSFGPrint',
                data: {
                    ExpDate: moment($('#PrintExpDate').val(), 'DD/MM/YYYY').format('YYYY-MM-DD'),
                    InDate: moment($('#PrintInDate').val(), 'DD/MM/YYYY').format('YYYY-MM-DD'),
