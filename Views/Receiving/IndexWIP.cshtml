                <div class="form-group row">
                    <label class="col-form-label col-sm-4"><strong>Material</strong></label>
                    <div class="col-sm-8">
                        &nbsp;
                    </div>
                </div>
                <div class="form-group row">
                    <label class="col-form-label col-sm-4">Code</label>
                    <div class="col-sm-7">
                        <input type="text" autocomplete="off" class="form-control" name="EditOrderMateCode" id="EditOrderMateCode" readonly />
                        <span class="help-block text-danger"></span>
                    </div>
                </div>
                <div class="form-group row">
                    <label class="col-form-label col-sm-4">Name</label>
                    <div class="col-sm-7">
                        <input type="text" autocomplete="off" class="form-control" name="EditOrderMateName" id="EditOrderMateName" readonly />
                        <span class="help-block text-danger"></span>
                    </div>
                </div>
                <div class="form-group row">
                    <label class="col-form-label col-sm-4">In Date</label>
                    <div class="col-sm-7">
                        <input type="text" autocomplete="off" class="form-control" id="EditOrderInDate" readonly />
                    </div>
                </div>
                <div class="form-group row">
                    <label class="col-form-label col-sm-4">Exp Date</label>
                    <div class="col-sm-7">
                        <input type="text" autocomplete="off" class="form-control" id="EditOrderExpDate" readonly />
                    </div>
                </div>
                <div class="form-group row">
                    <label class="col-form-label col-sm-4">Lot No</label>
                    <div class="col-sm-7">
                        <input type="text" autocomplete="off" class="form-control" id="EditOrderLotNo" readonly />
                    </div>
                </div>
                <div class="form-group row">
                    <label class="col-form-label col-sm-4">Total Order</label>
                    <div class="col-sm-7">
                        <input type="text" autocomplete="off" class="form-control number-non-decimal" name="EditOrderRequestQtyTotal" id="EditOrderRequestQtyTotal" />
                        <span class="help-block text-danger" id="error_EditOrderQTY"></span>
                    </div>
                </div>
                <input type="hidden" id="receiveLotNo" />
                <input type="hidden" id="receiveID" />
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-danger" data-dismiss="modal"><i class="ik ik-x-circle"></i>No</button>
                <button type="button" class="btn btn-primary" id="btnUpdateOrder"><i class="ik ik-check-circle"></i>Save</button>
            </div>
        </div>
    </div>
</div>
