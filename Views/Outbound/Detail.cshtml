    @Scripts.Render("~/Content/plugins/select2/dist/js/select2.min.js")
    @Scripts.Render("~/Content/plugins/inputmask/js/jquery.inputmask.bundle.js")

    <script type="text/javascript">

        let server = '@ViewBag.server';
        var isValid = true;
        let token = '@Session["token"].ToString()';
    let ID = '@ViewBag.Param';

    let WarehouseCode = "";

    let pickingTable = null;

        let productTable = null;
        let returnTable = null;

    $('.number1').inputmask("9{*}", { "placeholder": "" });
    //$('.number').inputmask("9{*}", { "placeholder": "" });
    $('.number').inputmask({ "placeholder": "", 'alias': 'decimal', 'groupSeparator': ',', 'autoGroup': true, 'digits': 2, 'digitsOptional': true });

    $('.number-non-decimal').inputmask({ "placeholder": "", 'alias': 'decimal', 'groupSeparator': ',', 'autoGroup': true, 'digits': 0, 'digitsOptional': false });

    $('#btnBack').click(function () {
        window.location.replace('@Url.Action("Index", "Outbound")')
    });

    //$('.number').inputmask({ "placeholder": "", 'alias': 'numeric', 'groupSeparator': ',', 'radixPoint': '.', 'autoGroup': true, 'digits': 2, 'digitsOptional': true });

     $('.number1').inputmask("9{*}", { "placeholder": "" });
        //$('.number').inputmask("9{*}", { "placeholder": "" });
     $('.number').inputmask({ "placeholder": "", 'alias': 'decimal', 'groupSeparator': ',', 'autoGroup': true, 'digits': 2, 'digitsOptional': true});

    $('.number-non-decimal').inputmask({ "placeholder": "", 'alias': 'decimal', 'groupSeparator': ',', 'autoGroup': true, 'digits': 0, 'digitsOptional': false });


    initData();


    function initData() {
            $.ajax({
                type: 'GET',
                async: false,
                dataType: "json",
                beforeSend: function (request) {
                    request.setRequestHeader('Token', token);
                },
                url: server + 'Api/Outbound/GetDataById?id=' + ID,
                success: function (response) {
                    isValid = response.status;
                    if (isValid) {
                        let data = response.data;
                        ID = data.ID;

                        let removeBtn = false;
                        let pickedCol = true;

                        $('#btnSave').hide();
                        $('#btnAddDetail').hide();
                        $('#buttonGroup').hide();

                        $('#Remarks').prop('readonly', true);

                        if (data.TransactionStatus == 'OPEN') {
                            $('#Status').addClass('badge badge-info');
                            removeBtn = true;
                            pickedCol = false;
                            $('#Remarks').prop('readonly', false);
                            $('#btnSave').show();
                            $('#btnAddDetail').show();
                            $('#buttonGroup').show();
                        } else if (data.TransactionStatus == 'CLOSED') {
                            $('#Status').addClass('badge badge-warning');
                            $('#pickingLog').show();
                        } else {
                            $('#Status').addClass('badge badge-success');
                            $('#pickingLog').show();
                            $('#btnClose').show();
                        }

                        WarehouseCode = data.WarehouseCode;

                        //init picking table

                        pickingTable = $('#pickingTable').DataTable({
                            processing: true,
                            serverSide: true,
                            scrollY: "500px",
                            scrollX: true,
                            scrollCollapse: true,
                            paging: true,
                            order: [[11, "desc"]],
                            ajax: {
                                "dataType": "json",
                                "type": "POST",
                                "url": server + "Api/Outbound/DatatablePicking?HeaderID=" + ID
                            },
                            columns: [
                                { "data": "BinRackCode", "name": "BinRackCode", "autoWidth": true },
                                { "data": "MaterialCode", "name": "MaterialCode", "autoWidth": true },
                                { "data": "MaterialName", "name": "MaterialName", "autoWidth": true },
                                { "data": "LotNo", "name": "LotNo", "autoWidth": true },
                                { "data": "InDate", "name": "InDate", "autoWidth": true },
                                { "data": "ExpDate", "name": "ExpDate", "autoWidth": true },
                                { "data": "BagQty", "name": "BagQty", "autoWidth": true },
                                { "data": "QtyPerBag", "name": "QtyPerBag", "autoWidth": true },
                                { "data": "TotalQty", "name": "TotalQty", "autoWidth": true },
                                { "data": "PickingMethod", "name": "PickingMethod", "autoWidth": true },
                                { "data": "PickedBy", "name": "PickedBy", "autoWidth": true },
                                { "data": "PickedOn", "name": "PickedOn", "autoWidth": true },
                            ]
                        });

                        returnTable = $('#returnSumTable').DataTable({
                            processing: true,
                            serverSide: true,
                            scrollY: "500px",
                            scrollX: true,
                            scrollCollapse: true,
                            paging: true,
                            order: [[9, "desc"]],
                            ajax: {
                                "dataType": "json",
                                "type": "POST",
                                "url": server + "Api/Outbound/DatatableReturn?HeaderID=" + ID
                            },
                            columns: [

                                { "data": "MaterialCode", "name": "MaterialCode", "autoWidth": true },
                                { "data": "MaterialName", "name": "MaterialName", "autoWidth": true },
                                { "data": "LotNo", "name": "LotNo", "autoWidth": true },
                                { "data": "InDate", "name": "InDate", "autoWidth": true },
                                { "data": "ExpDate", "name": "ExpDate", "autoWidth": true },
                                { "data": "QtyPerBag", "name": "QtyPerBag", "autoWidth": true },
                                { "data": "ReturnQty", "name": "ReturnQty", "autoWidth": true },
                                { "data": "ReturnMethod", "name": "ReturnMethod", "autoWidth": true },
                                { "data": "ReturnedBy", "name": "ReturnedBy", "autoWidth": true },
                                { "data": "ReturnedOn", "name": "ReturnedOn", "autoWidth": true },
                                {
                                    "data": null,
                                    "render": function (data, type, row) {
                                        let button = '';
                                        if (data.PutawayReturnAction) {
                                            button += '<button class="btn btn-info-outline" onclick = "return ShowBarcodeForm(this)" > <i class="fas fa-barcode"></i>Print</button> ';
                                            button += '<button type="button" class="btn btn-primary returnBtn" onclick="return ShowPutawayAction(this)"><i class="ik ik-inbox"></i>Putaway</button> ';
                                        }

                                        return button;
                                    },
                                },
                            ]
                        });

                        putawayTable = $('#putawaySumTable').DataTable({
                            processing: true,
                            serverSide: true,
                            scrollY: "500px",
                            scrollX: true,
                            scrollCollapse: true,
                            paging: true,
                            order: [[11, "desc"]],
                            ajax: {
                                "dataType": "json",
                                "type": "POST",
                                "url": server + "Api/Outbound/DatatablePutawayReturn?HeaderID=" + ID
                            },
                            columns: [

                                { "data": "MaterialCode", "name": "MaterialCode", "autoWidth": true },
                                { "data": "MaterialName", "name": "MaterialName", "autoWidth": true },
                                { "data": "LotNo", "name": "LotNo", "autoWidth": true },
                                { "data": "InDate", "name": "InDate", "autoWidth": true },
                                { "data": "ExpDate", "name": "ExpDate", "autoWidth": true },
                                { "data": "QtyPerBag", "name": "QtyPerBag", "autoWidth": true },
                                { "data": "PutawayQty", "name": "PutawayQty", "autoWidth": true },
                                { "data": "PutawayMethod", "name": "PutawayMethod", "autoWidth": true },
                                { "data": "BinRackCode", "name": "BinRackCode", "autoWidth": true },
                                { "data": "BinRackName", "name": "BinrackName", "autoWidth": true },
                                { "data": "PutBy", "name": "PutBy", "autoWidth": true },
                                { "data": "PutOn", "name": "PutOn", "autoWidth": true },


                            ]
                        });

                        $('#Status').html(data.TransactionStatus);

                        $('#Warehouse').val(data.WarehouseCode + ' - ' + data.WarehouseName);
                        $('#Remarks').val(data.Remarks);


                        $('#orderTable').DataTable({
                            processing: true,
                            serverSide: true,
                            scrollY: "500px",
                            scrollX: true,
                            scrollCollapse: true,
                            paging: true,
                            order: [[0, "desc"]],
                            ajax: {
                                "dataType": "json",
                                "type": "POST",
                                "url": server + "Api/Outbound/DatatableOrder?HeaderID=" + data.ID
                            },
                            columnDefs: [
                                { orderable: false, targets: -1 }
                            ],
                            columns: [
                                { "data": "MaterialCode", "name": "MaterialCode", "autoWidth": true },
                                { "data": "MaterialName", "name": "MaterialName", "autoWidth": true },
                                { "data": "MaterialType", "name": "MaterialType", "autoWidth": true },
                                { "data": "TotalQty", "name": "TotalQty", "autoWidth": true },
                                { "data": "PickedQty", "name": "PickedQty", "autoWidth": true, "visible": pickedCol },
                                { "data": "DiffQty", "name": "DiffQty", "autoWidth": true, "visible": pickedCol },
                                { "data": "CreatedBy", "name": "CreatedBy", "autoWidth": true },
                                { "data": "CreatedOn", "name": "CreatedOn", "autoWidth": true },
                                {
                                    "data": null,
                                    "render": function (data, type, row) {
                                        let button = '';
                                        if (removeBtn) {
                                            button += '<button type="button" class="btn btn-danger" onclick="return RemoveData(this)"><i class="ik ik-trash"></i>Remove</button> ';
                                        }
                                        if (pickedCol) {
                                            button += '<button type="button" class="btn btn-primary" onclick="return ShowPickingForm(this)"><i class="ik ik-inbox"></i>Picking</button> ';
                                        }
                                        if (data.ReturnAction) {
                                            button += '<button type="button" class="btn btn-primary returnBtn" onclick="return ShowReturnAction(this)"><i class="ik ik-inbox"></i>Return</button> ';
                                        }
                                        return button;
                                    },
                                    "visible" : data.TransactionStatus != 'CLOSED'
                                },
                            ]
                        });


                       productTable = $('#productTable').DataTable({
                            processing: true,
                            serverSide: true,
                            scrollY: "250px",
                            scrollX: true,
                            scrollCollapse: true,
                            paging: true,
