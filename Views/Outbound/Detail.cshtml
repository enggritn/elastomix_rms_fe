                        <div class="col-sm-6">
                            <input type="text" autocomplete="off" class="form-control" id="MaterialType" readonly />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-form-label col-sm-4">Available Qty</label>
                        <div class="col-sm-6">
                            <input type="text" autocomplete="off" class="form-control number" id="AvailableQty" readonly />
                        </div>
                        <div class="col-sm-1">
                            <label class="col-form-label"><b>KG</b></label>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-form-label col-sm-4">Request Qty</label>
                        <div class="col-sm-6">
                            <input type="text" autocomplete="off" class="form-control number" id="TotalQty" />
                            <span class="help-block text-danger"></span>
                        </div>
                        <div class="col-sm-1">
                            <label class="col-form-label"><b>KG</b></label>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" id="btnSaveDetail"><i class="ik ik-plus"></i>Save Detail</button>
            </div>
        </div>
    </div>
</div>

